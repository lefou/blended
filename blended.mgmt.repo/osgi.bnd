Bundle-Version: \
  ${project.version}

Bundle-SymbolicName: \
  ${bundle.symbolicName}; singleton:=true

Bundle-Activator: \
  ${bundle.namespace}.internal.ArtifactRepoActivator

Import-Package: \
  scala.*;version="${scalaImportVersion}", \
  *

Private-Package: \
  ${bundle.namespace}.file.*, \
  ${bundle.namespace}.internal.*

Export-Package: \
  ${bundle.namespace}
