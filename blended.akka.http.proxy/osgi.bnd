Bundle-Version:\
  ${project.version}

Bundle-SymbolicName:\
  ${bundle.symbolicName}; singleton:=true

Bundle-Activator:\
  ${bundle.namespace}.internal.BlendedAkkaHttpProxyActivator

Export-Package: \
  ${bundle.namespace}
  
Import-Package: \
  scala.*;version="[${scala.version},${scala.version}.50)", \
  *

Private-Package: \
  ${bundle.namespace}.internal
