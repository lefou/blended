Bundle-Version: \
  ${project.version}

Bundle-SymbolicName: \
  ${bundle.symbolicName}; singleton:=true

Import-Package: \
  scala.*;version="${scalaImportVersion}", \
  org.apache.commons.daemon;resolution:=optional, \
  de.tototec.cmdoption.*;resolution:=optional, \
  *

Export-Package: \
  ${bundle.namespace}

Private-Package: \
  ${bundle.namespace}.internal, \
  ${bundle.namespace}.jvmrunner, \
  ${bundle.namespace}.runtime
