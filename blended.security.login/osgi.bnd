Bundle-Version:\
  ${project.version}

Bundle-SymbolicName:\
  ${bundle.symbolicName}; singleton:=true

Bundle-Activator:\
  ${bundle.symbolicName}.internal.LoginActivator

Import-Package: \
  scala.*;version="${scalaImportVersion}", \
  android.*;resolution:=optional, \
  *

Private-Package:\
  ${bundle.namespace}.internal.*

Export-Package: \
  ${bundle.namespace},\
  ${bundle.namespace}.json
