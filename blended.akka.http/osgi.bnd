Bundle-Version:\
  ${project.version}

Bundle-SymbolicName:\
  ${bundle.symbolicName}; singleton:=true

Bundle-Activator:\
  ${bundle.namespace}.internal.BlendedAkkaHttpActivator

Export-Package: \
  ${bundle.namespace}
  
Import-Package: \
  scala.*;version="${scalaImportVersion}", \
  *

Conditional-Package: \
  blended.util.config

Private-Package: \
  ${bundle.namespace}.internal
