Bundle-Version: \
  ${project.version}

Bundle-SymbolicName: \
  ${bundle.symbolicName}; singleton:=true

Export-Package: \
  ${bundle.namespace}, \
  ${bundle.namespace}.protocol

Import-Package: \
  scala.*;version="${scalaImportVersion}", \
  *

Private-Package:\
  ${bundle.namespace}.internal.*

Bundle-Activator: \
  ${bundle.namespace}.internal.PersistenceActivator
