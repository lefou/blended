Bundle-Version:\
  ${project.version}

Bundle-SymbolicName:\
  ${bundle.symbolicName}; singleton:=true

Bundle-Activator: ${bundle.namespace}.internal.ContainerContextActivator

Import-Package: \
  scala.*;version="${scalaImportVersion}", \
  blended.launcher.runtime;resolution:=optional, \
  *

Export-Package: \
  ${bundle.namespace}

Private-Package: \
  ${bundle.namespace}.internal
