Bundle-Version:\
  ${project.version}

Bundle-SymbolicName:\
  ${bundle.symbolicName}; singleton:=true

Bundle-Activator: \
  ${bundle.namespace}.internal.ArtifactRepoActivator

Import-Package: \
  scala.*;version="[${scala.version},${scala.version}.50)", \
  *

Private-Package: \
  ${bundle.namespace}.file.*, \
  ${bundle.namespace}.internal.*

Export-Package: \
  ${bundle.namespace}
