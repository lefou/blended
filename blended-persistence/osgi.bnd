Bundle-Version:\
  ${project.version}

Bundle-SymbolicName:\
  ${bundle.symbolicName}; singleton:=true

Export-Package: \
  ${bundle.namespace}, \
  ${bundle.namespace}.protocol

Import-Package: \
  scala.*;version="[${scala.version},${scala.version}.50)", \
  *

Private-Package:\
  ${bundle.namespace}.internal.*

Bundle-Activator: \
  ${bundle.namespace}.internal.PersistenceActivator

