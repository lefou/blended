Bundle-Version:\
  ${project.version}

Bundle-SymbolicName: \
  ${bundle.symbolicName}; singleton:=true

Bundle-Activator: \
  ${bundle.namespace}.internal.BlendedUpdaterActivator

Import-Package: \
  scala.*;version="[${scala.version},${scala.version}.50)", \
  blended.launcher.runtime;resolution:=optional, \
  *

# Those will be embedded, if used
Conditional-Package: \
  blended.util.config

Export-Package: \
  ${bundle.namespace}

Private-Package: \
  ${bundle.namespace}.internal